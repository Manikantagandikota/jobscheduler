<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/trampoline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="HardcodedText"
        android:text="trampoline Schedule Job" />

    <Button
        android:id="@+id/newThread"
        android:layout_width="wrap_content"
        tools:ignore="HardcodedText"
        android:layout_height="wrap_content"
        android:text="newThread Schedule Job" />
    <Button
        android:id="@+id/computation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="HardcodedText"
        android:text="computation Schedule Job" />
    <Button
        android:id="@+id/io"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="io Schedule Job"
        tools:ignore="HardcodedText" />
    <Button
        android:id="@+id/from"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="HardcodedText"
        android:text="from Schedule Job" />
  </androidx.appcompat.widget.LinearLayoutCompat>